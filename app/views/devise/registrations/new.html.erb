<div class="panel panel-default">
 <div class="panel-heading"><h2>Rejestracja</h2></div>
  <div class="panel-body">




<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="form-group">
    <%= f.label :email, 'Podaj swój email' %><br />
    <%= f.email_field :email, autofocus: true, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :password, 'Podaj hasło' %>
    <% if @minimum_password_length %>
    <em>(minimum <%= @minimum_password_length %> znaków)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :password_confirmation, 'Potwiedź hasło' %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit "Wyślij formularz", class: 'btn btn-primary' %>
  </div>
<% end %>




   </div>
  <div class="panel-footer"><%= render "devise/shared/links" %></div> 
</div>


